<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.demo.models.CurrencyEntity" table="currency" schema="sbertech_db">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="day" column="day" type="java.sql.Date"/>
        <property name="time" column="time" type="java.sql.Time"/>
        <property name="euro" column="euro" type="java.lang.Float"/>
        <property name="dollar" column="dollar" type="java.lang.Float"/>
    </class>
</hibernate-mapping>